<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pages</title>
  </head>
  <body>
    <h1>Pages</h1>
    <ul>
      <% pages.forEach( page => { %>
        <li>
          <a href="/pages/<%= page._id %>" alt="id: <%= page._id %>">
             <%= page.title %>
          </a>
        </li>
      <% }) %>
    </ul>
  </body>
</html>
